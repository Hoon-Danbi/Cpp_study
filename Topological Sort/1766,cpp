//https://www.acmicpc.net/problem/1766
#include <iostream>
#include <vector>
#include <algorithm>
#include <queue>
using namespace std;

int main(){
    int N,M;
    cin>>N>>M;
    
    vector<vector<int>> map(N+1);
    vector<int> degree(N+1,0);

    for(int i=0;i<M;i++){
        int s,e;
        cin>>s>>e;
        degree[e]++;
        map[s].push_back(e);
    }

    priority_queue<int,vector<int>,greater<int>> pq;
    for(int i=1;i<N+1;i++){
        if(degree[i]==0) pq.push(i);
    }

    while(!pq.empty()){
        int cur=pq.top();pq.pop();
        cout<<cur<<" ";
        for(auto ch:map[cur]){
            if(--degree[ch] ==0) pq.push(ch);
        }
    }
    return 0;
    
    
    
}
